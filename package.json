{
  "name": "vite-template",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "start": "vite --host",
    "build": "tsc && vite build",
    "preview": "yarn build && vite preview",
    "new": "plop --plopfile generators/index.js",
    "lint": "eslint --cache",
    "storybook": "start-storybook ./ -p 6006",
    "test:update": "jest --updateSnapshot ./src",
    "test:watch": "jest ./src",
    "test:coverage": "jest --coverage --watchAll=false ./src",
    "pre-commit": "lint-staged"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --cache",
      "bash -c tsc"
    ]
  },
  "dependencies": {
    "@amfi/connect-wallet": "1.1.24",
    "@reduxjs/toolkit": "^1.6.2",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.2",
    "clsx": "^1.1.1",
    "jest": "^27.5.1",
    "node-sass": "^7.0.1",
    "rc-dialog": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-redux": "^7.2.1",
    "react-router-dom": "6",
    "react-toastify": "^8.1.0",
    "redux-persist": "^6.0.0",
    "redux-saga": "^1.1.3",
    "use-debounce": "^7.0.1",
    "uuid": "^8.3.2",
    "web3": "^1.2.11",
    "web3-utils": "^1.7.0"
  },
  "devDependencies": {
    "@esbuild-plugins/node-globals-polyfill": "^0.1.1",
    "@esbuild-plugins/node-modules-polyfill": "^0.1.4",
    "@mdx-js/preact": "^2.1.0",
    "@rollup/plugin-eslint": "^8.0.1",
    "@rollup/plugin-inject": "^4.0.4",
    "@rollup/plugin-url": "^6.1.0",
    "@storybook/addon-a11y": "^6.4.19",
    "@storybook/addon-actions": "^6.3.8",
    "@storybook/addon-essentials": "^6.3.8",
    "@storybook/addon-links": "^6.3.8",
    "@storybook/addon-storyshots": "^6.3.8",
    "@storybook/addon-storyshots-puppeteer": "^6.3.8",
    "@storybook/addons": "^6.3.8",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.8",
    "@svgr/rollup": "^6.2.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^12.1.0",
    "@testing-library/react-hooks": "^7.0.2",
    "@testing-library/user-event": "^13.2.1",
    "@types/jest": "^27.0.2",
    "@types/node": "^17.0.8",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@types/redux-persist": "^4.3.1",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.2",
    "dotenv": "^14.2.0",

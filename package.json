{
  "name": "vite-template",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "prepare": "husky install",
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "lint": "npm-run-all lint:*",
    "lint-fix": "npm-run-all format lint-fix:*",
    "format": "prettier --write \"{src,types,scripts}/**/*.{json,{j,t}s{,x},scss,}\"",
    "lint:ts": "eslint \"{src,types}/**/*.ts{,x}\"",
    "lint-fix:ts": "eslint \"{src,types}/**/*.ts{,x}\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\"",
    "lint-fix:scss": "stylelint \"src/**/*.scss\" --fix"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,css,scss,less}": [
      "prettier --ignore-path ./.prettierignore --write "
    ],
    "*.{js,jsx,ts,tsx}": [
      "eslint"
    ]
  },
  "dependencies": {
    "@amfi/connect-wallet": "^1.1.20",
    "axios": "^0.24.0",
    "bignumber.js": "^9.0.2",
    "classnames": "^2.3.1",
    "mobx": "^6.3.12",
    "mobx-react": "^7.2.1",
    "mobx-react-lite": "^3.2.3",
    "mobx-state-tree": "^5.1.0",
    "node-sass": "^7.0.1",
    "rc-dialog": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "6"
  },
  "devDependencies": {
    "@rollup/plugin-eslint": "^8.0.1",
    "@types/node": "^17.0.8",
    "@types/node-sass": "^4.11.2",
    "@types/react": "^17.0.33",
    "@types/react-dom": "^17.0.10",
    "@types/react-router-dom": "^5.3.2",
    "@typescript-eslint/eslint-plugin": "^5.9.1",
    "@typescript-eslint/parser": "^5.9.1",
    "@vitejs/plugin-legacy": "^1.6.4",
    "@vitejs/plugin-react": "^1.0.7",
    "@vitejs/plugin-react-refresh": "^1.3.6",
    "autoprefixer": "^10.4.2",
    "dotenv": "^14.2.0",
    "eslint": "^8.6.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-prettier": "^8.3.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-simple-import-sort": "^7.0.0",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.5",
    "postcss-scss": "^4.0.3",
    "prettier": "^2.5.1",
    "process": "^0.11.10",
    "sass": "^1.48.0",
    "stylelint": "^14.2.0",
    "stylelint-config-prettier": "^9.0.3",
    "stylelint-config-standard": "^24.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-prettier": "^2.0.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vite-plugin-eslint": "^1.3.0",
    "vite-tsconfig-paths": "^3.3.17"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
